{
  "version": "1.0",
  "local_store": "/var/lib/git2consul_cache",
  "logger": {
    "name": "git2consul",
    "streams": [
      {
        "level": "trace",
        "stream": "process.stdout"
      },
      {
        "level": "debug",
        "type": "rotating-file",
        "path": "/var/log/git2consul/git2consul.log"
      }
    ]
  },
  "repos": [
    {
      "name": "dev",
      "url": "https://github.com/cd871127/shit-code-cloud-infrastructure.git",
      "include_branch_name": false,
      "ignore_repo_name": true,
      "source_root": "config/dev/",
      "mountpoint": "config",
      "branches": [
        "dev"
      ],
      "hooks": [
        {
          "type": "polling",
          "interval": "1"
        }
      ]
    }
  ]
}